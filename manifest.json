{
   "chrome_url_overrides": {
      "history": "history.html"
   },
   "description": "Enhanced History",
   "icons": {
      "128": "css/icon.png"
   },
   "manifest_version": 3,
   "name": "iHistory",
   "permissions": [ "history", "favicon" ],
   "web_accessible_resources": [
      {
         "resources": ["_favicon/*"],
         "matches": ["<all_urls>"],
         "extension_ids": ["*"]
      }
   ],
   "version": "1.14"
}
